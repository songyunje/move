@RequestMapping("/rest3")
public class TestController3 {
	@GetMapping("/test1")
	public String test1() {
		return "GET";
	}
	@PostMapping("/test2")
	public String test2() {
		return "GET";
	}@PutMapping("/test3")
	public String test3() {
		return "GET";
	}@DeleteMapping("/test4")
	public String test4() {
		return "GET";
	}
}

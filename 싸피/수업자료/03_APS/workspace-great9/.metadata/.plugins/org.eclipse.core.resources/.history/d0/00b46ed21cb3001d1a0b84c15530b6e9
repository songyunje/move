package test05;

public class Test {
	// 내부 클래스
	static class Person{
		int num; // 사람의 번호
		int cnt; // 큐에서 나왔을 때 몇개를 가져갈지??
		
		Person(int num, int cnt){
			this.num = num;
			this.cnt = cnt;
		}
	}
	
	public static void main(String[] args) {
		int N = 20 ; // 마이쮸 개수
		int pNum = 1; // 사람 번호
	}
}



